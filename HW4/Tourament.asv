function [newcand, I] = Tourament( candidate, parents )
    shuffle = randperm(length(candidate));
    pairing = randperm(length(candidate / 2));
    rounds = parents / 2;
    
    for r = 1:rounds
        winners = [];
        for i = 1:length(pairing)
            cand1 = candidate(shuffle(i));
            cand2 = candidate(shuffle(i + pairing(i)));
            if cand1.fit > cand2.fit
                winners = [cand1 wi
                
            
        end
    end
end

